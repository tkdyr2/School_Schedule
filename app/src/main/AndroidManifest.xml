<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cookandroid.com.schoolschedule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/myroundicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResearchClassActivity"
            android:label="@string/research_frame_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BucketActivity"
            android:label="@string/bucket_frame_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyScheduleActivity"
            android:label="@string/result_frame_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisteredScheduleActivity"
            android:label="@string/RegisteredSchedule_frame_name"
            android:screenOrientation="portrait" />

    </application>

</manifest>