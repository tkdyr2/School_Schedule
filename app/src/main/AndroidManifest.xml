<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cookandroid.com.schoolschedule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ScheduleResultActivity"
            android:label="@string/result_frame_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResearchClassActivity"
            android:label="@string/research_frame_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BucketActivity"
            android:label="@string/bucket_frame_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyResultActivity"
            android:label="@string/my_frame_result"
            android:screenOrientation="portrait" />
    </application>

</manifest>