<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/origin_backGray"
    tools:context=".ScheduleResultActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TableLayout
            android:id="@+id/timeTable1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">
        </TableLayout>
        <TableLayout
            android:id="@+id/timeTable2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">


        </TableLayout>
        <TableLayout
            android:id="@+id/timeTable3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">
        </TableLayout>
        <TableLayout
            android:id="@+id/timeTable4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">


        </TableLayout>
        <TableLayout
            android:id="@+id/timeTable5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">

        </TableLayout>
        <TableLayout
            android:id="@+id/timeTable6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">


        </TableLayout>
        <TableLayout
            android:id="@+id/timeTable7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp"
            android:layout_weight="1"
            android:background="@color/origin_liteGray"
            android:textAlignment="center">

        </TableLayout>


    </LinearLayout>
</ScrollView>
